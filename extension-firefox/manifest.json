{
  "manifest_version": 2,
  "name": "eBird Trip Report â†’ CSV",
  "version": "0.1.0",
  "description": "Export an eBird trip report species list to CSV from the rendered page.",
  "content_scripts": [
    {
      "matches": ["https://ebird.org/tripreport/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "ebird-tripreport-to-csv@hydrospheric0.local",
      "strict_min_version": "140.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    },
    "gecko_android": {
      "strict_min_version": "142.0"
    }
  }
}
